class TuioObjectCreatedTrigger < EventTriggers

  def initialize( session_id, fiducial_id, x_pos, y_pos )
    @session_id = session_id
    @fiducial_id = fiducial_id
    @x_pos = x_pos
    @y_pos = y_pos
  end
  
  def match?( event )
    if event.kind_of?( TuioEvent )
      (event.fiducial_id == @fiducial_id)
    else
      false
    end
  end

end
